<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />

	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_collection_left}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_collection_left}.pdf</pdf-file-name>

	<pdf-report pageSize="letter" rotate="false" footer="true" header="true">
		<record recordset='query.sql' title='${lbl:b_collection_left}' width='60' spacingBefore='40'>
			<col name='cco_codigo' title='${lbl:b_code}' align='left' />
			<col name='cco_descripcion' title='${lbl:b_description}' align='left' />
			<col name='smn_rol_id_combo' title='${lbl:b_role}' align='left' format='######' />
			<col name='smn_auxiliar_id_combo' title='${lbl:b_auxiliary}' align='left' format='######' />
			<col name='rol_estatus' title='${lbl:b_status}' align='left' />
			<col name='rol_idioma' title='${lbl:s_profile_lang}' align='left' />
			<col name='rol_usuario' title='${lbl:b_users}' align='left' />
			<col name='rol_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
			<col name='rol_hora' title='${lbl:s_loginhist_col2}' align='left' />
		</record>




	</pdf-report>
</config>
